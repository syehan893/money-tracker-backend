{
  "name": "finance-os-backend",
  "version": "1.0.0",
  "description": "Finance OS - Comprehensive financial management backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "finance",
    "budgeting",
    "expense-tracker",
    "income-tracker",
    "typescript",
    "express",
    "supabase"
